# .env.example (Staging environment variables)

# API key for staging service
STAGING_API_KEY=your-staging-api-key

# OpenSearch configuration for staging
STAGING_OPENSEARCH_HOST=https://your-staging-opensearch-host/user-profiles/_search
STAGING_OPENSEARCH_USER=your-opensearch-username
STAGING_OPENSEARCH_PWD=your-opensearch-password

# Anthropic (Claude) API key for staging
ANTHROPIC_STAGING_API_KEY=your-anthropic-staging-api-key

# Health Profile Group endpoint for staging
STAGING_HEALTH_PROFILE_GROUP=https://your-staging-healthprofile-group-endpoint

# Claude (Anthropic) API key (duplicate)
CLAUDE_API_KEY=your-claude-api-key

# OpenAI staging API key
STAGING_OPENAI_API_KEY=your-openai-staging-api-key
